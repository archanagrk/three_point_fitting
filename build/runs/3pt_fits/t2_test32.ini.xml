<ThreeptIniParams>

    <inputProps>
        <NumdbFiles>1</NumdbFiles>
        <dbFnames>
            <elem>
                <name>t32.edb</name>
                <dt>32</dt>
                <tmin>0</tmin>
                <tmax>32</tmax>
                <tsrc>0</tsrc>
                <tsnk>32</tsnk>
                <EiMin>0.01</EiMin> <!-- Approximately the diff in the energy of the ground state & 1st ex. state at src -->
                <EfMin>0.01</EfMin> <!-- Diff in the energy of the ground state & 1st ex. state at sink -->
                <EiMax>0.8</EiMax>  <!-- Dominated by the first ex. state but limit till first threshold -->
                <EfMax>0.4</EfMax>
                <NtMin>5</NtMin> 
            </elem>
        </dbFnames>
    </inputProps>


    <FitProps>
        <fit_qual>
            <type>generic</type>
              <params>
              <power>1</power>
                <powerExp>1</powerExp>
                <powerT>2</powerT>
                <expFac>true</expFac>
              </params>
        </fit_qual>
        <divKfac>true</divKfac>
        <kfac>check.xml</kfac>
        <chisq_cutoff>2</chisq_cutoff>
        <noise_cutoff>100</noise_cutoff>
        <Ei>
          <elem>rho_proj0_p110_H1D2B2.jack</elem> <!--should be consistent with the name of the operator used in the 3pt function-->
          <elem>rho_proj0_p000_T1.jack</elem>
          <elem>rho_proj0_p100_H1D4E2.jack</elem>
          <elem>rho_proj0_p110_H1D2B1.jack</elem>
        </Ei>  
        <Ef>
          <elem>pion_proj0_p110_H0D2A2.jack</elem>
          <elem>pion_proj0_p000_A1.jack</elem>
          <elem>pion_proj0_p100_H0D4A2.jack</elem>
        </Ef>
    </FitProps>

</ThreeptIniParams>

